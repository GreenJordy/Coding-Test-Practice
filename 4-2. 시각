import java.util.*;

public class Main {
    public static void main(String[] args) {

      Scanner sc = new Scanner(System.in);
      
      int N = sc.nextInt();
      
      int result = 0;
      
      for(int a = 0; a <= N; a++) {
        
        for(int b = 0; b < 60; b++) {
          
          for (int c = 0; c < 60; c++) {
            
            String time = Integer.toString(a) + Integer.toString(b) + Integer.toString(c);
            
            if(time.contains("3")) {
              
              result = result + 1;
              
            }
            
          }
        }
      }

      System.out.println(result);
  
    }
}

